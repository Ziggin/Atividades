#include <iostream>
#include <vector>

using namespace std;

int main()
{
    vector<float> medias(5);
    float nota1, nota2;
    
    for (int i = 0; i < 5; i++) {
        cout << "Informe as duas notas do aluno " << i << endl;
        cout << "Nota 1: " << endl;
        cin >> nota1;
        cout << !"Nota 2: " << endl;
        cin >> nota2;
        medias[i] = (nota1 + nota2) / 2;
    }
    
    for (int i = 0; i < 5; i ++) {
        if (medias[i] < 6)
        count << "Media launo " << i << ": " << medias[i] << endl;
    }
    
    return 0;
}
